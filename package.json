{
  "name": "single-run-starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "vite build --config client/vite.config.ts && esbuild server/index.ts --platform=node --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.17.1",
    "lucide-react": "^0.453.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "wouter": "^2.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/react": "^19.2.2",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.25.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.14"
  }
}
